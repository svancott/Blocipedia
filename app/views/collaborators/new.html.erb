<div class="row">
  <div class="col-md-4">
    <p>Please enter the collaborator:</p>
  </div>
  <div class="col-md-8">
    <%= form_for @collaborator do |f| %>
      <div class="form-group">
        <%= f.label :wiki_id %>
        <%= f.text_field :wiki_id, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :user_id %>
        <%= f.text_field :user_id, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
